<script type="text/javascript">
   var count = 2;
function run() {
var un = document.myform.email.value;
var pw = document.myform.pass.value;
var valid = false;
var unArray = ["piyush@pikashu.com", "karan@pikashu.com", "shubham@pikashu.com"];  // as many as you like - no comma after final entry
var pwArray = ["piyush", "karan", "shubham"];  // the corresponding passwords;
for (var i=0; i <unArray.length; i++) {
if ((un == unArray[i]) && (pw == pwArray[i])) {
valid = true;
break;
}
}
if (valid ) {
  if(un=="piyush@pikashu.com" && pw=="piyush")
  {
alert ("Login was sucessfully processed.  You will be redirected to the members page now.");
window.location = "homepika.html";
return false;
}
else if(un=="karan@pikashu.com" && pw=="karan")
  {
alert ("Login was sucessfully processed.  You will be redirected to the members page now.");
window.location = "homepika1.html";
return false;
}
if(un=="shubham@pikashu.com" && pw=="shubham")
  {
alert ("Login was sucessfully processed.  You will be redirected to the members page now.");
window.location = "homepika2.html";
return false;
}
}
var t = " tries";
if (count == 1) {t = " try"}
if (count >= 1) {
alert ("Invalid username and/or password.  You have " + count + t + " left.");
document.myform.username.value = "";
document.myform.pword.value = "";
setTimeout("document.myform.username.focus()", 25);
setTimeout("document.myform.username.select()", 25);
count --;
}
else {
alert ("Still incorrect! You have no more tries left!");
document.myform.username.value = "No more tries allowed!";
document.myform.pword.value = "";
document.myform.username.disabled = true;
document.myform.pword.disabled = true;
return false;
}
}


  // Get the modal
  var modal = document.getElementById('id01');

  // When the user clicks anywhere outside of the modal, close it
  window.onclick = function(event) {
    if (event.target == modal) {
        modal.style.display = "none";
    }
  }
  
    // Get the modal
  var hi = document.getElementById('id02');

  // When the user clicks anywhere outside of the modal, close it
  window.onclick = function(event) {
    if (event.target == hi) {
        modal.style.display = "none";
    }
  }
</script>
